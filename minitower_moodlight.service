[Unit]
Description=Minitower moodlight Service
DefaultDependencies=no
StartLimitIntervalSec=60
StartLimitBurst=5
[Service]
RootDirectory=/ 
User=root
Type=simple
xecStart=/bin/bash -c '/usr/bin/python3 home/pi/RaspberryPiNAS/moodlight.py -c &'
RemainAfterExit=yes
Restart=always
RestartSec=30
[Install]
WantedBy=multi-user.target
